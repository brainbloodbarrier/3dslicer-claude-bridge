[run]
source = src/slicer_mcp
branch = True
omit =
    */tests/*
    */__pycache__/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
fail_under = 85
show_missing = True
precision = 2

[html]
directory = htmlcov
